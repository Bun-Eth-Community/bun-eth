{
  "name": "@bun-eth/burner-connector",
  "version": "0.1.0",
  "description": "Burner wallet connector for quick local testing",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --external wagmi --external viem",
    "dev": "tsup src/index.ts --format cjs,esm --watch --external wagmi --external viem"
  },
  "dependencies": {
    "wagmi": "^2.16.4",
    "viem": "^2.7.8"
  },
  "devDependencies": {
    "@types/node": "^20.11.5",
    "tsup": "^8.0.1",
    "typescript": "^5.6.2"
  },
  "keywords": ["ethereum", "burner", "wallet", "testing", "wagmi"],
  "license": "MIT"
}
